/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  position:relative;
  background: #dedede;
} 

.box_container{
	height: 1200rpx;
}
.footer_container{
	display: flex;
  justify-content: space-between;
	position: fixed;
	height: 100rpx;
	width: 100%;
	background: #ffffff;
  border-top:solid 1px #cccccc;
	bottom: 0;
	left:0;
}