/**底部样式**/

.foot1 image{
  width: 86rpx;
  height: 60rpx;
}
.foot2 image{
  width: 120rpx;
  height: 130rpx;
  margin-top: -60rpx;
}
.foot3 image{
  width: 44rpx;
  height: 72rpx;
}